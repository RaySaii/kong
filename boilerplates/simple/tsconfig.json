{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "lib": [
      "es6",
      "es5",
      "dom",
      "es2017",
      "es2018",
      "esnext"
    ],
    "moduleResolution": "node",
    "jsx": "react",
    "baseUrl": "..",
    "allowJs": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules"
  ]
}
